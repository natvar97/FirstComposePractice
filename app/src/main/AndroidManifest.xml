<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.firstcomposepractice">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FirstComposePractice">
        <activity
            android:name=".ComposeConstraintLayout"
            android:exported="true"
            android:label="@string/title_activity_compose_constraint_layout"
            android:theme="@style/Theme.FirstComposePractice.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ComplexCustomLayout"
            android:exported="true"
            android:label="@string/title_activity_complex_custom_layout"
            android:theme="@style/Theme.FirstComposePractice.NoActionBar">

        </activity>
        <activity
            android:name=".LayoutComposable"
            android:exported="true"
            android:label="@string/title_activity_layout_composable"
            android:theme="@style/Theme.FirstComposePractice.NoActionBar" />
        <activity
            android:name=".ExtendModifier"
            android:exported="true"
            android:label="@string/title_activity_extend_modifier"
            android:theme="@style/Theme.FirstComposePractice.NoActionBar" />
        <activity
            android:name=".ScaffoldExample"
            android:exported="true"
            android:label="@string/title_activity_scaffold_example"
            android:theme="@style/Theme.FirstComposePractice.NoActionBar" />
        <activity
            android:name=".JetpackComposeLayoutCodelab"
            android:exported="true"
            android:label="@string/title_activity_jetpack_compose_layout_codelab"
            android:theme="@style/Theme.FirstComposePractice.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.FirstComposePractice.NoActionBar" />
    </application>

</manifest>